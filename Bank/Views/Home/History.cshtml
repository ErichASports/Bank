<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>

  <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.2.1.min.js"></script>
</head>
<body>
		<label id="label">Transaction History</label>
		<output id="trans" name="history">0</output>
    
		<script type="text/javascript">
        $(document).ready(function() 
        {   
			<!--get the valuses of the deposits and withdrawls-->
            var storedBal = JSON.parse(localStorage.getItem("aryBal"));

			<!--get the username-->
			var name = localStorage.username;
            $("#label").text(name + "'s " + "Transaction History");
			<!--reverse the order so most recent transactions appear first-->
			var recentBal = storedBal.reverse();

			<!--move the values to the output field-->
			$("output").text(recentBal);

			});
		</script>
</body>
</html>
